{
    "name": "Start Clean",
    "description": "Launches apps, the good way!",
    "version": "0.1",
    "manifest_version": 2,
    "permissions" : ["management", "storage"],
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    },
    "content_security_policy": "script-src 'self' https://www.google.com https://www.googleapis.com 'unsafe-eval'; object-src 'self'",
    "icons": { "48": "icon.png" },
    "options_page": "options.html"
}
